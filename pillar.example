awscli:
  users:
    john:
      # homedir is optional, by default it will use /home/username
      homedir: '/home/jsmith'
      config:
        region: 'us-east-1'
        aws_access_key_id: 'ACCESS_KEY_ID'
        aws_secret_access_key: 'SECRET_ACCESS_KEY'
    will:
      config:
        region: 'us-west-1'
        aws_access_key_id: 'ACCESS_KEY_ID_WILL'
        aws_secret_access_key: 'SECRET_ACCESS_KEY_WILL'

